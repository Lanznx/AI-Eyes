{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.",
      "**/node_modules/**"
    ],
    "cleanUrls": false,
    "rewrites": [
      {
        "source": "/sw.js",
        "destination": "/sw.js"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
